    meta(http-equiv="X-UA-Compatible" content="ie=edge")
    link(href="/stylesheet/style.css" rel="stylesheet" type="text/css")
    link(href="https://fonts.googleapis.com/css?family=Open+Sans&display=swap" rel="stylesheet")
